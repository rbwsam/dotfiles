[core]
	editor = vim
	excludesfile = ~/.gitignoreglobal
[color]
	ui = true
[user]
	email = REPLACE_ME
	name = Sam Leavens
[alias]
	lg = log --decorate --date=local
	st = status
	c = commit
	diffc = diff --cached
[url "git@github.com:"]
	insteadOf = https://github.com/
[branch]
	autoSetupMerge = always
[push]
	default = current
